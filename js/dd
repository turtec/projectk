        'A#/2':46,
        'BB/2':46,
        'B/2' :47,
        'B#/2':48,
        'CB/3':47,
        'C/3' :48,
        'C#/3':49,
        'DB/3':49,
        'D/3' :50,
        'D#/3':51,
        'EB/3':51,
        'E/3' :52,
        'E#/3':53,
        'FB/3':52,
        'F/3' :53,
        'F#/3':54,
        'GB/3':54,
        'G/3' :55,
        'G#/3':56,
        'AB/3':56,
        'A/3' :57,
