        'B#/3':60,
        'CB/4':59,
        'C/4' :60,
        'C#/4':61,
        'DB/4':61,
        'D/4' :62,
        'D#/4':63,
        'EB/4':63,
        'E/4' :64,
        'E#/4':65,
        'FB/4':64,
        'F/4' :65,
        'F#/4':66,
